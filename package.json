{
  "name": "chums-checkin",
  "main": "expo-router/entry",
  "version": "1.3.1",
  "scripts": {
    "start": "expo start --tunnel",
    "start-local": "expo start --localhost",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx expo run:android",
    "android-safe": "npx cross-env WATCHMAN_DISABLE=1 CI=true EXPO_NO_TELEMETRY=1 REACT_EDITOR=none expo run:android --port 8085 --no-install",
    "android-windows": "set WATCHMAN_DISABLE=1 && set CI=true && npx expo run:android --port 8085",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:android": "eas build --platform android",
    "build:amazon": "eas build --platform android --profile amazon",
    "build:manual": "node build-android-manual.js",
    "install:clean": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install --legacy-peer-deps",
    "install:force": "npm install --legacy-peer-deps --force"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@churchapps/mobilehelper": "0.3.14",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.17",
    "expo-asset": "^11.1.7",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.79.5",
    "react-native-device-info": "^14.0.4",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-material-ripple": "^0.9.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-snackbar": "^2.9.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/js": "^9.30.1",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/metro-config": "^0.80.1",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native-material-ripple": "^0.9.6",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3"
  },
  "overrides": {
