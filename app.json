{
  "expo": {
    "name": "Chums Checkin",
    "slug": "chums-checkin",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "churchapps",
    "extra": {
      "eas": {
        "projectId": "3e543eda-7610-49dc-b345-38234d7b9059"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.chums.checkin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/images/logo1.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/3e543eda-7610-49dc-b345-38234d7b9059"
    },
    "ios": {
      "bundleIdentifier": "org.chums.checkin"
    },
    "runtimeVersion": "1.0.0"
  }
}
